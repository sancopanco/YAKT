
<div id="head">
   <%= render 'inactive_users' %>
   <%= time_ago_in_words(@card.created_at) %> ago |
	    <%= link_to "Edit", edit_card_path(@card) %> |
   <a id="add_card" href="<%=newsubcard_card_path(@card)%>">add card</a>
</div>
<br/><br/>
<table class="states table table-striped">
<tbody>	
  <tr class="success">	
    <% @states.each do |state| %>
      <%=render state%>
    <% end %>
  </tr>
</tbody> 
</table>
<%= javascript_include_tag "card" %>
<%= javascript_include_tag "user" %>